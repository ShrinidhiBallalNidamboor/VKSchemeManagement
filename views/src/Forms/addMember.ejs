          <br>
          <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Add Member</h4>
                  <form action="/addMember" method="POST">
                    <div class="form-group">
                      <label for="Username">Username</label>
                      <input type="text" class="form-control" placeholder="Username" name="username" required>
                    </div>
                    <div class="form-group">
                      <label for="PhoneNumber">Registration ID</label>
                      <input type="text" class="form-control" placeholder="Registration ID" name="registrationID" required>
                    </div>
                    <div class="form-group">
                      <label for="PhoneNumber">Phone Number 1</label>
                      <input type="text" class="form-control" placeholder="Phone Number" name="phonenumber1" required>
                    </div>
                    <div class="form-group">
                      <label for="PhoneNumber">Phone Number 2</label>
                      <input type="text" class="form-control" placeholder="Phone Number" name="phonenumber2" required>
                    </div>
                    <div class="form-group">
                      <label for="agentID">Agent Code</label>
                      <input type="text" class="form-control" placeholder="Agent Code" name="agentcode">
                    </div>
                    <div class="form-group">
                      <label for="agentID">Executive Code</label>
                      <input type="text" class="form-control" placeholder="Executive Code" name="execuitivecode">
                    </div>
                    <div class="form-group">
                      <label for="agentID">Season Number</label>
                      <input type="number" class="form-control" placeholder="Season Number" name="seasonnumber" required>
                    </div>
                    <button type="submit" class="btn btn-primary me-2">Add</button>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
